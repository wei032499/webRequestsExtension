{
    "description": "Modify webRequests",
    "icons": {
        "96": "icons/icon-96.jpg"
    },
    "browser_action": {
        "default_icon": "icons/icon-96.jpg",
        "default_title": "Modify webRequests"
    },
    "manifest_version": 2,
    "name": "webRequest-modify",
    "version": "1.0",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "storage",
        "<all_urls>"
    ],
    "options_ui": {
        "page": "options/options.html",
        "open_in_tab": true
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "{c81d212d-bf38-4f9b-8491-865ca232568e}",
            "strict_min_version": "42.0"
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}